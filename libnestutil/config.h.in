#define SLI_PATCHLEVEL "@SLI_PATCHLEVEL@"

// TODO SLI_HOST and SLI_HOSTVENDOR not available with cmake
#define SLI_HOST "@CMAKE_HOST_SYSTEM_PROCESSOR@-@host_vendor@-@CMAKE_HOST_SYSTEM@"
